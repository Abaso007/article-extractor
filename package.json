    "coveralls": "npm run report && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "async": "latest",
    "bellajs": "latest",
    "bluebird": "latest",
    "cheerio": "latest",
    "read-art": "latest",
    "node-fetch": "latest",
    "node-readability": "latest",
    "oembed-auto-es6": "latest",
    "sanitize-html": "latest"
  },
  "devDependencies": {
    "chance": "latest",
    "coveralls": "latest",
    "nyc": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "article",
    "extractor",
    "parser",
    "cleaner",
    "crawler",
    "readability",
    "oembed",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
