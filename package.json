{
  "version": "0.5.10",
  "name": "article-parser",
  "description": "Extract clean article data from given URL.",
  "homepage": "http://techpush.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/article-parser"
  },
  "author": "Dong Nguyen <ndaidong@gmail.com> (https://twitter.com/ndaidong)",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "test": "snyk test && ./node_modules/.bin/tape test/start.js | tap-spec",
    "coverage": "./node_modules/.bin/nyc tape test/start.js | tap-spec",
    "report": "npm run coverage && ./node_modules/.bin/nyc report --reporter=lcov",
    "coveralls": "npm run report && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "snyk": "./node_modules/.bin/snyk wizard"
  },
  "dependencies": {
    "bellajs": "latest",
    "cheerio": "latest",
    "jsdom": "latest",
    "node-fetch": "latest",
    "promise-wtf": "latest",
    "sanitize-html": "latest"
  },
  "devDependencies": {
    "chance": "latest",
    "coveralls": "latest",
    "nyc": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "article",
    "extractor",
    "parser",
    "cleaner",
    "crawler",
    "readability",
    "oembed",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/article-parser/issues"
  },
  "directories": {
    "test": "test"
  }
}